<mj-group mj-class="footer-group"
          css-class="footer-group">

  <mj-column mj-class="footer__column"
             css-class="footer__column"
             width="45%">

    <mj-image mj-class="footer__logo logo"
              css-class="footer__logo logo"
              src="https://i.postimg.cc/TYW4cJp2/img-guitar-logo.png"
              href="https://www.google.com/"
              alt="GuitarLogo"
              title="Go To Guitar"
              width="120px" />

  </mj-column>

  <mj-column mj-class="footer__column"
             css-class="footer__column"
             width="55%">

    <mj-social mj-class="footer__social"
               css-class="footer__social">

        <mj-social-element src="https://i.postimg.cc/8c7n9h4H/img-guitar-facebook.png"
                           href="https://www.google.com/"
                           alt="facebook"
                           title="facebook"></mj-social-element>

        <mj-social-element src="https://i.postimg.cc/wjX4CxXS/img-guitar-instagram.png"
                           href="https://www.google.com/"
                           alt="instagram"
                           title="instagram"></mj-social-element>

        <mj-social-element src="https://i.postimg.cc/xCCsNKw2/img-guitar-twitter.png"
                           href="https://www.google.com/"
                           alt="twitter"
                           title="twitter"></mj-social-element>

    </mj-social>

  </mj-column>

</mj-group>
